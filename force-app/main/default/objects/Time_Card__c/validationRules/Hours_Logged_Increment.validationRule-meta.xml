<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hours_Logged_Increment</fullName>
    <active>true</active>
    <errorConditionFormula>IF(MOD(  Hours_Logged__c * 100, 25 ) &lt;&gt; 0 , TRUE, FALSE )</errorConditionFormula>
    <errorDisplayField>Hours_Logged__c</errorDisplayField>
    <errorMessage>Enter Time in increments of .25</errorMessage>
</ValidationRule>
